config BCMDHD
	tristate "Broadcom 4329/30 wireless cards support"
	---help---
	  This module adds support for wireless adapters based on
	  Broadcom 4329/30 chipset.

config BCMDHD_DISABLE_OOB
	depends on BCMDHD
	bool "disable Out-of-Band Interrupt"
	---help---
		disable Interrupt from WL_HOST_WAKE.

config BCMDHD_AP6335
	tristate "Broadcom AP6335 wireless cards support"
	---help---
	  This module adds support for wireless adapters based on
	  Broadcom AP6335 module chipset.

